{
	"name": "TalkPresidential",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"android": "node node_modules/react-native/local-cli/cli.js run-android",
		"ios": "node node_modules/react-native/local-cli/cli.js run-ios",
		"prebuild": "rm -rf build",
		"build": "tsc",
		"watch": "npm run build -- -w",
		"lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
		"pretest": "npm run lint",
		"test": "jest",
		"build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'"
	},
	"dependencies": {
		"@types/core-js": "^0.9.43",
		"@types/react-native": "^0.50.2",
		"@types/react-navigation": "^1.0.22",
		"@types/react-redux": "^5.0.12",
		"babel-preset-es2015": "^6.24.1",
		"emoji-regex": "^6.5.1",
		"lodash": "^4.17.4",
		"numeral": "2.0.6",
		"react": "^16.6.0-alpha.8af6728",
		"react-native": "^0.57.3",
		"react-native-fbsdk": "^0.6.3",
		"react-native-firebase": "^3.2.4",
		"react-native-push-notification": "^3.1.2",
		"react-navigation": "^1.0.0-beta.19",
		"react-number-format": "3.2.0",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2",
		"redux-actions": "^2.2.1",
		"redux-persist": "^5.3.5",
		"redux-persist-transform-encrypt": "^2.0.0",
		"redux-saga": "^0.16.0",
		"reselect": "^3.0.1",
		"talisman": "^0.20.0"
	},
	"devDependencies": {
		"@types/jest": "^21.1.6",
		"babel-jest": "21.2.0",
		"babel-preset-react-native": "4.0.0",
		"jest": "21.2.1",
		"metro-react-native-babel-preset": "^0.53.1",
		"react-test-renderer": "16.0.0-beta.5",
		"redux-devtools": "^3.4.0",
		"remote-redux-devtools": "^0.5.12",
		"ts-jest": "^21.2.2",
		"tslint": "^5.8.0",
		"typescript": "^2.6.1",
		"webpack": "^3.8.1"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
			"^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
		},
		"testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
	}
}
